  "bugs": {
    "url": "https://github.com/chriswoodle/blokta/issues"
  },
  "homepage": "https://github.com/chriswoodle/blokta#readme",
  "dependencies": {
    "dotenv": "^7.0.0",
    "source-map-support": "^0.5.11"
  }
}
